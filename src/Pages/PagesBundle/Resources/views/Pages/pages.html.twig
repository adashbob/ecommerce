{% extends ":layout:layout.html.twig" %}

{% block body %}
    <div class="container">
        <div class="row">
            {% for page in pages %}
                <h1>{{ page.titre }}</h1>
                <div class="span12">
                    {{ page.contenu|raw }}
                </div>
                <hr>
            {% endfor %}
        </div>
    </div>
{% endblock %}